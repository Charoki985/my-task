postgres=# SELECT books.title,
postgres-# books.author,
postgres-# rentals.borrower,
postgres-# rentals.rental_date
postgres-# FROM rentals
postgres-# JOIN books
postgres-# ON rentals.book_id = books.id
postgres-# WHERE rentals.returned = '未返却';
